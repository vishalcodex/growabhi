{
    "name": "xendit/xendit-php",
    "description": "PHP clients for Xendit API",
    "version": "2.19.0",
    "license": "MIT",
    "keywords": [
        "xendit"
    ],
    "homepage": "https://github.com/xendit/xendit-php",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3.0",
        "guzzlehttp/guzzle": ">=7.5.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Xendit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xendit\\": [
                "tests/",
                "tests/Xendit/"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.7.2",
        "vlucas/phpdotenv": "^4.3"
    }
}
